{
  "name": "dev-kit",
  "version": "1.0.0",
  "description": "[Model Context Protocol] Dev Kit - anything a developer need for his day to day works",
  "keywords": [
    "MCP",
    "gitlab"
  ],
  "author": "munichbughunter",
  "license": "MIT",
  "type": "module",
  "bin": "./build/index.js",
  "files": [
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "deploy": "npm publish --access public"
  },
  "dependencies": {
    "@gitbeaker/rest": "^42.4.0",
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@types/json-schema": "^7.0.15",
    "axios": "^1.8.4",
    "zod": "^3.24.3",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
